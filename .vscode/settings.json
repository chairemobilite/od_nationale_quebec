{
    "i18n-ally.localesPaths": [
        "survey/locales"
    ],
    "i18n-ally.enabledFrameworks": [
        "react",
        "i18next"
    ],
    "i18n-ally.enabledParsers": [
        "json", "yaml"
    ],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.review.enabled": false,
    "i18n-ally.namespace": true,
    "i18n-ally.pathMatcher": "{locale}/{namespaces}.{ext}",
    "i18n-ally.indent": 4
}